    "react": "^19.1",
    "react-dom": "^19.1",
    "react-redux": "^9.2.0",
    "react-router": "^7.1.5"
  },
  "peerDependenciesMeta": {
    "@shlinkio/shlink-js-sdk": {
      "optional": true
    }
  },
  "dependencies": {
    "@formkit/drag-and-drop": "^0.5.3",
