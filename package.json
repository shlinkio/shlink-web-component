    "@shlinkio/eslint-config-js-coding-standard": "~3.7.0",
    "@shlinkio/shlink-js-sdk": "^2.2.2",
    "@stylistic/eslint-plugin": "^5.4.0",
    "@tailwindcss/vite": "^4.1.14",
    "@testing-library/jest-dom": "^6.9.1",
