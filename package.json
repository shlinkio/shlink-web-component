{
  "name": "@shlinkio/shlink-web-component",
  "description": "Minimal UI to interact with Shlink on React applications",
  "private": false,
  "repository": "https://github.com/shlinkio/shlink-web-component",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./api-contract": {
      "import": "./dist/api-contract.js",
      "require": "./dist/api-contract.cjs",
      "types": "./dist/api-contract.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "test": "vitest run",
    "test:ci": "npm run test -- --coverage",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:css": "stylelint src/*.scss src/**/*.scss dev/*.scss dev/**/*.scss",
    "lint:js": "eslint --ext .ts,.tsx dev src test",
    "lint:fix": "npm run lint:css:fix && npm run lint:js:fix",
    "lint:css:fix": "npm run lint:css -- --fix",
    "lint:js:fix": "npm run lint:js -- --fix",
    "types": "tsc",
    "dev": "vite serve --host 0.0.0.0 --port 3002",
    "dev:sub-route": "vite serve --host 0.0.0.0 --port 3003 --base=\"/sub/route\""
  },
  "peerDependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-brands-svg-icons": "^6.4.2",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@reduxjs/toolkit": "^2.0.1",
    "@shlinkio/shlink-frontend-kit": "^0.5.1",
    "@shlinkio/shlink-js-sdk": "^1.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.0.1",
    "react-router-dom": "^6.20.1",
    "reactstrap": "^9.2.0"
  },
  "peerDependenciesMeta": {
    "@shlinkio/shlink-js-sdk": {
      "optional": true
    }
  },
  "dependencies": {
    "@formkit/drag-and-drop": "^0.0.38",
    "@json2csv/plainjs": "^7.0.6",
    "@shlinkio/data-manipulation": "^1.0.3",
    "bottlejs": "^2.0.1",
    "bowser": "^2.11.0",
    "clsx": "^2.1.1",
    "compare-versions": "^6.1.0",
    "date-fns": "^3.6.0",
    "event-source-polyfill": "^1.0.31",
    "leaflet": "^1.9.4",
    "react-external-link": "^2.3.1",
    "react-leaflet": "^4.2.1",
    "react-swipeable": "^7.0.1",
    "react-tag-autocomplete": "^7.2.0",
    "recharts": "^2.12.7"
  },
  "devDependencies": {
    "@shlinkio/eslint-config-js-coding-standard": "~2.4.1",
    "@shlinkio/stylelint-config-css-coding-standard": "~1.1.1",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.2",
    "@total-typescript/shoehorn": "^0.1.2",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/leaflet": "^1.9.12",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.6.0",
    "axe-core": "^4.9.0",
    "bootstrap": "5.2.3",
    "eslint": "^9.2.0",
    "history": "^5.3.0",
    "jsdom": "^24.0.0",
    "resize-observer-polyfill": "^1.5.1",
    "sass": "^1.77.1",
    "stylelint": "^15.11.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^1.2.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
